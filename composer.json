{
    "name": "barryvdh/laravel-elfinder",
    "keywords": ["laravel", "elfinder", "tinymce", "filebrowser"],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fuqu-io/elfinder-flysystem-driver"
    }
  ],
    "require": {
        "studio-42/elfinder": "~2.1.10",
        "barryvdh/elfinder-flysystem-driver": "dev-master as 0.2.x-dev",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\Elfinder\\ElfinderServiceProvider"
            ]
        }
    }
}
